# Based on https://github.com/cmu-db/peloton/blob/484d76df9344cb5c153a2c361c5d5018912d4cf4/test/sql/distinct_sql_test.cpp

statement ok
CREATE TABLE test (a INT PRIMARY KEY, b INT, c INT, d VARCHAR);

statement ok
INSERT INTO test VALUES
    (1, 22, 333, 'abcd'),
    (2, 22, 333, 'abc'),
    (3, 11, 222, 'abcd');

# DistinctIntTest
query I rowsort
SELECT DISTINCT b FROM test;
----
11
22

# DistinctVarcharTest
query T rowsort
SELECT DISTINCT d FROM test;
----
abc
abcd

# DistinctTupleTest
query II rowsort
SELECT DISTINCT b, c FROM test;
----
11 222
22 333

# DistinctStarTest

statement ok
DROP TABLE test;

statement ok
CREATE TABLE test (
    id INTEGER PRIMARY KEY,
    a INT,
    b INT,
    c INT,
    d VARCHAR
);

statement ok
INSERT INTO test VALUES
    (1, 1, 22, 333, 'abcd'),
    (2, 1, 22, 333, 'abcd'),
    (3, 1, 22, 222, 'abcd');

query IIIT rowsort
SELECT DISTINCT a, b, c, d FROM test;
----
1 22 222 abcd
1 22 333 abcd

# DistinctDateTimeTest

statement ok
DROP TABLE test;

statement ok
CREATE TABLE test(id INTEGER PRIMARY KEY, a INT, b TEXT);

statement ok
