# AggregateGroupByManyAVGsSQLTest
query RRRRR rowsort
SELECT
    CAST(avg(a) AS REAL),
    CAST(avg(b) AS REAL),
    CAST(avg(c) AS REAL),
    CAST(avg(c) AS REAL),
    CAST(avg(c) AS REAL)
FROM test
GROUP BY d;
