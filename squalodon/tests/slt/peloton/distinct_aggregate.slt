# DistinctAggregateCountTest
query IIIIIIIII
SELECT
    count(*),
    count(DISTINCT s),
    count(DISTINCT i),
    count(DISTINCT bi),
    count(DISTINCT r),
    count(DISTINCT t),
    count(DISTINCT bp),
    count(DISTINCT vc),
    count(DISTINCT ts)
